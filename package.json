{
    "name": "@sveu/extend",
    "version": "0.2.0",
    "packageManager": "pnpm@7.20.0",
    "description": "A collection of 3rd party integrations for Svelte",
    "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
    "license": "MIT",
    "homepage": "https://github.com/svelte-u/extend#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svelte-u/extend.git"
    },
    "bugs": {
        "url": "https://github.com/svelte-u/extend/issues"
    },
    "keywords": [
        "svelte",
        "integration"
    ],
    "type": "module",
    "scripts": {
        "build": "pnpm run clean && tsx scripts/build.ts && tsup",
        "clean": "tsx scripts/clean.ts",
        "git:add": "pnpm run build && pnpm run clean && git add .",
        "commit": "cz commit",
        "format": "prettier --plugin-search-dir . --write .",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp --all",
        "test": "vitest run",
        "test:watch": "vitest --watch"
    },
    "exports": {
        "./fuse": {
            "import": "./fuse.js"
        },
        "./idb_keyval": {
            "import": "./idb_keyval.js"
        },
        "./jwt": {
            "import": "./jwt.js"
        },
        "./qrcode": {
            "import": "./qrcode.js"
        }
    },
    "peerDependencies": {
        "fuse.js": "*",
        "idb-keyval": "*",
        "jwt-decode": "*",
        "qrcode": "*"
    },
    "peerDependenciesMeta": {
        "fuse.js": {
            "optional": true
        },
        "idb-keyval": {
            "optional": true
        },
        "jwt-decode": {
            "optional": true
        },
        "qrcode": {
            "optional": true
        }
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.0.0",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^18.13.0",
        "@types/qrcode": "^1.5.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "bumpp": "^8.2.1",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "fast-glob": "^3.2.12",
        "fs-extra": "^11.1.0",
        "fuse.js": "^6.6.2",
        "idb-keyval": "^6.2.0",
        "jsdom": "^21.1.0",
        "jwt-decode": "^3.1.2",
        "prettier": "^2.8.4",
        "qrcode": "^1.5.1",
        "svelte": "^3.55.1",
        "tsup": "^6.6.3",
        "tsx": "^3.12.3",
        "typescript": "^4.9.5",
        "vitest": "^0.28.5"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@sveu/shared": "^0.7.0"
    }
}
