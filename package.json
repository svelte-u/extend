{
	"name": "@sveu/extend",
	"version": "0.0.1",
	"packageManager": "pnpm@7.20.0",
	"description": "A collection of 3rd party integrations for Svelte",
	"author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
	"license": "MIT",
	"homepage": "https://github.com/svelte-u/extend#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svelte-u/extend.git"
	},
	"bugs": {
		"url": "https://github.com/svelte-u/extend/issues"
	},
	"keywords": [
		"svelte",
		"integration"
	],
	"type": "module",
	"scripts": {
		"build": "pnpm run clean && tsx scripts/build.ts && tsup",
		"clean": "tsx scripts/clean.ts",
		"git:add": "pnpm run build && pnpm run clean && git add .",
		"commit": "cz commit",
		"lint": "eslint .",
		"lint:fix": "pnpm run lint --fix",
		"release": "bumpp --all",
		"test": "vitest run",
		"test:watch": "vitest --watch"
	},
	"exports": {},
	"peerDependencies": {},
	"peerDependenciesMeta": {},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"bumpp": "^8.2.1",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"fast-glob": "^3.2.12",
		"fs-extra": "^11.1.0",
		"prettier": "^2.8.1",
		"svelte": "^3.55.0",
		"tsup": "^6.5.0",
		"tsx": "^3.12.1",
		"typescript": "^4.9.4",
		"vitest": "^0.26.2"
	},
	"publishConfig": {
		"access": "public"
	}
