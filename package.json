{
    "name": "@sveu/extend",
    "version": "0.4.0",
    "packageManager": "pnpm@7.20.0",
    "description": "A collection of 3rd party integrations for Svelte",
    "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
    "license": "MIT",
    "homepage": "https://github.com/svelte-u/extend#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svelte-u/extend.git"
    },
    "bugs": {
        "url": "https://github.com/svelte-u/extend/issues"
    },
    "keywords": [
        "svelte",
        "integration"
    ],
    "type": "module",
    "scripts": {
        "build": "pnpm run clean && tsx scripts/build.ts && tsup",
        "clean": "tsx scripts/clean.ts",
        "git:add": "pnpm run build && pnpm run clean && git add .",
        "commit": "cz commit",
        "format": "prettier --plugin-search-dir . --write .",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp --all",
        "test": "vitest run",
        "test:watch": "vitest --watch"
    },
    "exports": {
        "./async_validator": {
            "import": "./async_validator.js"
        },
        "./change_case": {
            "import": "./change_case.js"
        },
        "./fcm": {
            "import": "./fcm.js"
        },
        "./fuse": {
            "import": "./fuse.js"
        },
        "./idb_keyval": {
            "import": "./idb_keyval.js"
        },
        "./jwt": {
            "import": "./jwt.js"
        },
        "./qrcode": {
            "import": "./qrcode.js"
        },
        "./sortable": {
            "import": "./sortable.js"
        }
    },
    "peerDependencies": {
        "async-validator": "*",
        "change-case": "*",
        "firebase": "*",
        "fuse.js": "*",
        "idb-keyval": "*",
        "jwt-decode": "*",
        "qrcode": "*",
        "sortablejs": "*"
    },
    "peerDependenciesMeta": {
        "async-validator": {
            "optional": true
        },
        "change-case": {
            "optional": true
        },
        "firebase": {
            "optional": true
        },
        "fuse.js": {
            "optional": true
        },
        "idb-keyval": {
            "optional": true
        },
        "jwt-decode": {
            "optional": true
        },
        "qrcode": {
            "optional": true
        },
        "sortablejs": {
            "optional": true
        }
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^18.15.3",
        "@types/qrcode": "^1.5.0",
        "@types/sortablejs": "^1.15.1",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "async-validator": "^4.2.5",
        "bumpp": "^8.2.1",
        "change-case": "^4.1.2",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "fast-glob": "^3.2.12",
        "firebase": "^9.18.0",
        "fs-extra": "^11.1.0",
        "fuse.js": "^6.6.2",
        "idb-keyval": "^6.2.0",
        "jsdom": "^21.1.1",
        "jwt-decode": "^3.1.2",
        "prettier": "^2.8.4",
        "qrcode": "^1.5.1",
        "sortablejs": "^1.15.0",
        "svelte": "^3.57.0",
        "tsup": "^6.6.3",
        "tsx": "^3.12.5",
        "typescript": "^4.9.5",
        "vitest": "^0.29.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@sveu/shared": "^0.9.0"
    }
}
